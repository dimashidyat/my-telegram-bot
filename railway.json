{
    "schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS"
    },
    "deploy": {
        "startCommand": "python main.py",
        "healthcheckPath": "/",
        "healthcheckTimeout": 300,
        "restartPolicyType": "ALWAYS"
    },
    "env": {
        "PYTHON_VERSION": "3.11.7",
        "TZ": "Asia/Jakarta"
    }
}
